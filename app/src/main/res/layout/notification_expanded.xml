<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="180dp"
    android:layout_width="match_parent"
    android:orientation="horizontal"
    android:weightSum="10"
    android:background="@color/colorPink"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="180dp"
        android:orientation="vertical"
    android:layout_weight="7"
    >
        <LinearLayout
            android:layout_width="350dp"
            android:layout_gravity="center"
            android:layout_height="50dp"
            android:gravity="center"
            android:orientation="vertical"
            android:background="@drawable/padding"
            >
            <TextView
                android:id="@+id/conten"
                android:textColor="@color/white"
                android:textSize="10sp"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="fffff"/>
            <TextView
                android:id="@+id/nn"
                android:textColor="@color/white"
                android:textSize="10sp"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="fffff"/>
        </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:paddingTop="5dp"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center|top"
            android:orientation="vertical"
            android:layout_weight="1">

            <ImageView
                android:id="@+id/image_view_expanded1"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/icnotification1"
                android:scaleType="centerCrop"></ImageView>
            <TextView
                android:id="@+id/tv00"
                android:textColor="@android:color/white"
                android:layout_width="wrap_content"
                android:layout_height="12dp"
                android:text="Đau đầu"
                android:textSize="8sp" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center|top"
            android:orientation="vertical"
            android:weightSum="2"
            android:layout_weight="1">

            <ImageView

                android:id="@+id/image_view_expanded2"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/iconlogo"
                android:scaleType="centerCrop" />

            <TextView
                android:id="@+id/tv01"
                android:layout_width="wrap_content"
                android:layout_height="12dp"
                android:textColor="@android:color/white"
                android:text="Mụn"
                android:textSize="8sp" />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center|top"
            android:orientation="vertical"
            android:weightSum="2"
            android:layout_weight="1">

            <ImageView
                android:id="@+id/image_view_expanded3"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/iconlogo"
                android:scaleType="centerCrop" />

            <TextView
                android:id="@+id/tv02"
                android:layout_width="wrap_content"
                android:layout_height="12dp"
                android:text="Mệt mỏi"
                android:textColor="@android:color/white"
                android:textSize="8sp" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center|top"
            android:orientation="vertical"
            android:weightSum="2"
            android:layout_weight="1">

            <ImageView
                android:id="@+id/image_view_expanded4"
                android:layout_weight="1"
                android:layout_width="45dp"
                android:layout_height="45dp"
                app:srcCompat="@drawable/iconlogo"
                android:scaleType="centerCrop" />

            <TextView
                android:id="@+id/tv03"
                android:layout_width="wrap_content"
                android:layout_height="12dp"
                android:text="Tức ngực"
                android:textColor="@android:color/white"
                android:textSize="8sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center|top"
            android:orientation="vertical"
            android:weightSum="2"
            android:layout_weight="1">

            <ImageView
                android:id="@+id/image_view_expanded5"
                android:layout_weight="1"
                android:layout_width="45dp"
                android:layout_height="45dp"
                app:srcCompat="@drawable/iconlogo"
                android:scaleType="centerCrop" />

            <TextView
                android:id="@+id/tv04"
                android:layout_width="wrap_content"
                android:layout_height="12dp"
                android:text="Ngứa"
                android:textColor="@android:color/white"
                android:textSize="8sp" />

        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:paddingTop="5dp"
        android:orientation="horizontal">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center|top"
            android:orientation="vertical"
            android:weightSum="2"
            android:layout_weight="1">

            <ImageView
                android:id="@+id/image_view_expanded6"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/iconlogo"
                android:scaleType="centerCrop" />

            <TextView
                android:id="@+id/tv05"
                android:layout_width="wrap_content"
                android:layout_height="12dp"
                android:text="Buồn"
                android:textColor="@android:color/white"
                android:textSize="8sp" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center|top"
            android:orientation="vertical"
            android:weightSum="2"
            android:layout_weight="1">

            <ImageView
                android:id="@+id/image_view_expanded7"
                android:layout_weight="1"
                android:layout_width="45dp"
                android:layout_height="45dp"
                app:srcCompat="@drawable/iconlogo"
                android:scaleType="centerCrop" />

            <TextView
                android:id="@+id/tv06"
                android:layout_width="wrap_content"
                android:layout_height="12dp"
                android:text="Tức ngực"
                android:textColor="@android:color/white"
                android:textSize="8sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center|top"
            android:orientation="vertical"
            android:weightSum="2"
            android:layout_weight="1">

            <ImageView
                android:id="@+id/image_view_expanded8"
                android:layout_weight="1"
                android:layout_width="45dp"
                android:layout_height="45dp"
                app:srcCompat="@drawable/iconlogo"
                android:scaleType="centerCrop" />

            <TextView
                android:id="@+id/tv07"
                android:layout_width="wrap_content"
                android:layout_height="12dp"
                android:text="Ngứa"
                android:textColor="@android:color/white"
                android:textSize="8sp" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center|top"
            android:orientation="vertical"
            android:weightSum="2"
            android:layout_weight="1">

            <ImageView
                android:id="@+id/image_view_expanded9"
                android:layout_weight="1"
                android:layout_width="45dp"
                android:layout_height="45dp"
                app:srcCompat="@drawable/iconlogo"
                android:scaleType="centerCrop" />

            <TextView
                android:id="@+id/tv08"
                android:layout_width="wrap_content"
                android:layout_height="12dp"
                android:text="Táo bón"
                android:textColor="@android:color/white"
                android:textSize="8sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center|top"
            android:orientation="vertical"
            android:weightSum="2"
            android:layout_weight="1">

            <ImageView
                android:id="@+id/image_view_expanded10"
                android:layout_weight="1"
                android:layout_width="45dp"
                android:layout_height="45dp"
                app:srcCompat="@drawable/iconlogo"
                android:scaleType="centerCrop"
                />
            <TextView
                android:id="@+id/tv09"
                android:layout_width="wrap_content"
                android:layout_height="12dp"
                android:textColor="@android:color/white"
                android:text="Dịch lạ"
                android:textSize="8sp" />

        </LinearLayout>

    </LinearLayout>

</LinearLayout>
</LinearLayout>


<!--</androidx.appcompat.widget.LinearLayoutCompat>-->




